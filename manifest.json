{
  "manifest_version": 2,
  "name": "My Cool Extension",
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["jquery-1.11.2.min.js","content.js"]
  }],
  "background": {
  "scripts": ["background.js"]
  },
  "permissions": ["tabs"],
  "web_accessible_resources": ["jquery-1.11.2.min.js","shame.html","shame.js","shame.wav"]
}